# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Model Configuration
MODEL=gemini-2.5-flash
DEV_MODEL=gemini-2.5-pro

# GitHub Configuration (for repository creation and deployment)
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username

# Render Configuration (for deployment)
RENDER_API_KEY=your_render_api_key_here

# QA Configuration
QA_MODE=fast
FAST_QA_TIMEOUT=60
DEEP_QA_TIMEOUT=90
QA_CONFIDENCE_PASS=0.8
QA_CONFIDENCE_FLAG=0.5
QA_MAX_CODE_CHARS=3000
QA_TOTAL_CODE_LIMIT=15000

# Database Configuration (Phase 2 - Production Enhancement)
# PostgreSQL connection URL format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://user:password@localhost:5432/agenticai

# Optional: Use SSL for production
# DATABASE_URL=postgresql://user:password@host:5432/agenticai?sslmode=require
